<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.example.android.airenthalpymollier">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".EnthalpyMenuActivity"
            android:label="@string/enthalpy_calculator"/>
        <activity
            android:name=".AbsoluteHumidityActivity"
            android:label="@string/absolute_humidity_calculator"/>
        <activity
            android:name=".HeatOfProcessActivity"
            android:label="@string/heat_of_process_calculator"/>
        <activity
            android:name=".TemperatureConverterMenuActivity"
            android:label="@string/temperature_menu"/>
        <activity
            android:name=".BernoulieEquationMenuActivity"
            android:label="@string/bernoullis_equation_menu"/>
        <activity
            android:name=".PerfectFluidsBernoullisEquation"
            android:label="@string/perfect_fluids_bernoullis_equation_menu"/>
        <activity
            android:name=".ReynoldsNumberActivity"
            android:label="@string/reynolds_number"/>
        <activity
            android:name=".ActualFluidsBernoullisEquationMenuActivity"
            android:label="@string/actual_fluids"/>
        <activity
            android:name=".DarcyWeisbachEquationActivity"
            android:label="@string/darcy_weisbach_equation"/>
        <activity
            android:name=".LocalFlowResistanceActivity"
            android:label="@string/local_flow_resistance"/>
        <activity
            android:name=".DarcyFrictionFactorActivity"
            android:label="@string/darcy_friction_factor"/>
        <activity
            android:name=".HeatFlowResistanceCalculatorActivity"
            android:label="@string/heat_flow_resistance_calculator"
            android:parentActivityName=".MainActivity"
            tools:targetApi="jelly_bean"/>

        <activity
            android:name=".InfoMenuActivity"
            android:label="@string/info_button"/>
        <activity
            android:name=".AddHeatFlowResistanceMenuActivity"
            android:label="@string/add_heat_flow_resistance"/>

    </application>

</manifest>